<!--New Book部分テンプレート-->

<h2>New book</h2>

<%= form_with model: Book.new do |f| %>


    <div><label>Title</label></div>

    <div><%= f.text_field :title %></div>

    <div><label>Option</label></div>

    <div><%= f.text_area :body %></div>

    <div><%= f.submit 'Create Book' %></div>
<% end %>

